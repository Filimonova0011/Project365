<Window x:Class="Project365.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project365"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <TextBlock FontSize="15" HorizontalAlignment="Center" VerticalAlignment="Top" Text="Государственное бюджетное профессиональное образовательное учреждение "></TextBlock>
        <TextBlock FontSize="15" HorizontalAlignment="Center" VerticalAlignment="Center" Text="«Южно-Уральский Государственный колледж» "></TextBlock>

        <Frame NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered" Name="MainFrame" Grid.Row="1"></Frame>
        <Button Content="Back" Name="BtnBack" Width="175" HorizontalAlignment="Right" Margin="5" Click="BtnBack_Click"></Button>
        
    </Grid>
</Window>
